class circle
{
	double radius;
	//in java constructor can also have return type ,in that case it would be
	//interpreted as a function
	void circle()
	{
		System.out.println("hellie/");
	}
	circle()
	{
		radius=1.0;
	}
   circle(double r)
   {
	   radius = r;
   }
  void display()
  {
	  System.out.println(radius);
  }
}
public class constructor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    circle c1=new circle();
    circle c2=new circle(3.5);
    c1.display();
    c2.display();
    c1.circle();
    
	}
}
